            for(int y = 0; y < 40; y++)
            {
                for(int x = 0; x < 60; x++)
                {
                    if((case_x == x)&&(case_y == y))
                    {
                        if (mon_tab[y][x] == 1)
                        {
                            state = 1;
                        }
                        else
                            state = 0;
                    }
                }
            }
            if(state == 1)
            {
                blit(not_good, buffer1,0,0, case_x*16, case_y*15, SCREEN_W, SCREEN_H);

